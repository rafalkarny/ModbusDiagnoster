    <system:String x:Key="PropertiesToolTip">Communication properties</system:String>
    <system:String x:Key="StartPoolingToolTip">Start requesting modbus</system:String>
    <system:String x:Key="StopPoolingToolTip">Stop requesting modbus</system:String>
    <system:String x:Key="ConnectToolTip">Connect/Disconnect TCP Session</system:String>
    <system:String x:Key="ChangeConTCPpropertiesToolTip">Apply changes (reconnect device)</system:String>
    <system:String x:Key="SaveChangesToolTip">Save changes</system:String>
    <system:String x:Key="ExportToolTip">Export variables to CSV file</system:String>
    
    <!-- Labels -->
    <system:String x:Key="SerialPortLbl">Port:</system:String>
    <system:String x:Key="BitrateLbl">Bitrate:</system:String>
    <system:String x:Key="ParityLbl">Parity:</system:String>
    <system:String x:Key="StopBitsLbl">Stop bits:</system:String>
    <system:String x:Key="DTRLbl">DTR:</system:String>
    <system:String x:Key="OnLbl">On:</system:String>
    <system:String x:Key="OffLbl">Off:</system:String>
    <system:String x:Key="OnTxLbl">On TX:</system:String>
    <system:String x:Key="RTSLbl">RTS:</system:String>
    <system:String x:Key="defaultPath">C:\</system:String>
    <system:String x:Key="nameOf">Name:</system:String>
    <system:String x:Key="defaltNameOf">_ExportedFromModbusDiagnoster</system:String>
    <system:String x:Key="selectRegistersToExport">Select registers to export</system:String>
    <system:String x:Key="exportBtn">Export</system:String>
    <system:String x:Key="additionalDelayBetwenRequests">Additional delay between requests in miliseconds:</system:String>
    <system:String x:Key="maxNumberOfhrAndirRegisters">Maximum number of registers in single request for HR and IR:</system:String>
    <system:String x:Key="maxNumberOfdiAndcoils">Maximum number of registers in singl request for DI and Coils:</system:String>

    <!-- Errors -->
    <system:String x:Key="dirExists">Sorry but this device name already exists, please slect new name </system:String>
    <system:String x:Key="promptForAcceptTodelete">Are you sure?</system:String>
    <system:String x:Key="wrongTimestampValue">Wrong Timestamp value, please add correct numeric value</system:String>
    <system:String x:Key="wrongAdditionalDelayValue">Wrong value, please add correct numeric value</system:String>
    <system:String x:Key="wrongNumberOfRegistersValue">Wrong value, please add correct numeric value from 1-125 for Holding Registers and Input Registers</system:String>
    <system:String x:Key="wrongNumberOfBinaryValue">Wrong value, please add correct numeric value from 1-2000 for Discrete Inputs and Coils</system:String>
    <system:String x:Key="timeoutInfo">No response in scheduled time</system:String>
    
    <!--
    <system:String x:Key="forHRandIR">For Holding Registers and Input Registers:</system:String>
    <system:String x:Key="forDIandCoils">For Coils and Discrete Inputs:</system:String>
    <system:String x:Key="selectOutputAttributes">Select columns:</system:String>
    -->
    
    
</ResourceDictionary>